#: controller.py:73
msgid "HTML Headers"
msgstr "HTML Kopf"

#: plugin.py:31
msgid ""
"Plugin adds wiki-commands (:title:), (:description:), (:keywords:) and (:"
"htmlhead:)"
msgstr ""
"Plugin fügt Wiki-Befehle (:title:), (:description:), (:keywords:) und (:"
"htmlhead:) ein"

#: plugin.py:33
msgid ""
"<b>Usage:</b>\n"
"(:title Page title:)\n"
"\n"
"(:description Page description:)\n"
"\n"
"(:keywords keyword_1, keyword_2, other keyword:)\n"
"\n"
"(:htmlhead:)\n"
"&lt;meta http-equiv='Content-Type' content='text/html; charset=utf-8' /&gt;\n"
"\n"
"&lt;meta name='robots' content='index,follow' /&gt;\n"
"(:htmlheadend:)\n"
msgstr ""
"<b>Usage:</b>\n"
"(:title Page title:)\n"
"\n"
"(:description Page description:)\n"
"\n"
"(:keywords keyword_1, keyword_2, other keyword:)\n"
"\n"
"(:htmlhead:)\n"
"&lt;meta http-equiv='Content-Type' content='text/html; charset=utf-8' /&gt;\n"
"\n"
"&lt;meta name='robots' content='index,follow' /&gt;\n"
"(:htmlheadend:)\n"

#: plugin.py:54
msgid "https://jenyay.net/Outwiker/HtmlHeadsEn"
msgstr "https://jenyay.net/Outwiker/HtmlHeadsEn"

#: actions.py:32
msgid "Title (:title ...:)"
msgstr "Titel (:Titel ...:)"

#: actions.py:36
msgid ""
"HtmlHeads plugin. Insert (:title... :) command to add <title> tag in the "
"page head."
msgstr ""
"HtmlHeads Plugin. Einfügen (:Titel... :) Befehl <title> -Tag in der "
"Seitenkopfzeile hinzufügen."

#: actions.py:50
msgid "Description (:description ...:)"
msgstr "Beschreibung (:description ...:)"

#: actions.py:54
msgid ""
"HtmlHeads plugin. Insert (:description... :) command to add description in "
"the page head."
msgstr ""
"HtmlHeads Plugin. Einfügen (:description... :) Befehl um Beschreibung in der "
"Seitenkopfzeile hinzuzufügen."

#: actions.py:68
msgid "Keywords (:keywords ...:)"
msgstr "Schlüsselwörter (:keywords ...:)"

#: actions.py:72
msgid ""
"HtmlHeads plugin. Insert (:keywords... :) command to add keywords in the "
"page head."
msgstr ""
"HtmlHeads Plugin. Einfügen (:keywords... :) Befehl um Schlüsselwörter in der "
"Seitenkopfzeile hinzuzufügen."

#: actions.py:86
msgid "Custom head (:htmlhead:)"
msgstr "Benutzerdefinierte Kopf (:htmlhead:)"

#: actions.py:90
msgid ""
"HtmlHeads plugin. Insert (:htmlhead:) command to add custom tags in the page "
"head."
msgstr ""
"HtmlHeads Plugin. Einfügen (:htmlhead:) Befehl um Benutzerdefinierten Tag in "
"der Seitenkopfzeile hinzufügen."

#: actions.py:104
msgid "CSS Style (:style:)"
msgstr "CSS Stil (:style:)"

#: actions.py:108
msgid ""
"HtmlHeads plugin. Insert (:style:) command to add custom CSS style to page."
msgstr ""
"HtmlHeads Plugin. Einfügen (:style:) Befehl um Benutzerdefinierten CSS Stil "
"in der Seite hinzufügen."

