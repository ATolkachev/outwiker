#: controller.py:73
msgid "HTML Headers"
msgstr "HTML-huvud"

#: plugin.py:31
msgid ""
"Plugin adds wiki-commands (:title:), (:description:), (:keywords:) and (:"
"htmlhead:)"
msgstr ""
"Insticket lägger till wiki-kommandona (:title:), (:description:), (:"
"keywords:) och (:htmlhead:)"

#: plugin.py:33
msgid ""
"<b>Usage:</b>\n"
"(:title Page title:)\n"
"\n"
"(:description Page description:)\n"
"\n"
"(:keywords keyword_1, keyword_2, other keyword:)\n"
"\n"
"(:htmlhead:)\n"
"&lt;meta http-equiv='Content-Type' content='text/html; charset=utf-8' /&gt;\n"
"\n"
"&lt;meta name='robots' content='index,follow' /&gt;\n"
"(:htmlheadend:)\n"
msgstr ""
"<b>Användning:</b>\n"
"(:title Sidtitel:)\n"
"\n"
"(:description Sidbeskrivning:)\n"
"\n"
"(:keywords nyckelord_1, nyckelord_2, annat nyckelord:)\n"
"\n"
"(:htmlhead:)\n"
"&lt;meta http-equiv='Content-Type' content='text/html; charset=utf-8' /&gt;\n"
"\n"
"&lt;meta name='robots' content='index,follow' /&gt;\n"
"(:htmlheadend:)\n"

#: plugin.py:54
msgid "https://jenyay.net/Outwiker/HtmlHeadsEn"
msgstr "https://jenyay.net/Outwiker/HtmlHeadsEn"

